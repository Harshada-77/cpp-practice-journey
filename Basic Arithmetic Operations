/*
Program Name : Basic Arithmetic Operations  
Description  : Takes two numbers and performs +, -, *, /, and % operations.  
Input        : Two integers  
Output       : Results of each operation  

Sample Run   :  
Enter first number = 10  
Enter second number = 3  
13  
7  
30  
3  
1  
*/

#include <iostream>
using namespace std;

int main() {
    int x, y;
    cout << "Enter first number = ";
    cin >> x;
    cout << "Enter second number = ";
    cin >> y;
    cout << x + y << "\n";
    cout << x - y << "\n";
    cout << x * y << "\n";
    cout << x / y << "\n";
    cout << x % y << "\n";
    return 0;
}
