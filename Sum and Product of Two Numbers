/*
Program Name : Sum and Product of Two Numbers  
Description  : Takes two numbers and displays their sum and product.  
Input        : Two integers  
Output       : Sum and Product  

Sample Run   :  
Enter first number = 3  
Enter second number = 4  
7  
12  
*/

#include <iostream>
using namespace std;

int main() {
    int x, y;
    cout << "Enter first number = ";
    cin >> x;
    cout << "Enter second number = ";
    cin >> y;
    cout << x + y << "\n";
    cout << x * y;
    return 0;
}
